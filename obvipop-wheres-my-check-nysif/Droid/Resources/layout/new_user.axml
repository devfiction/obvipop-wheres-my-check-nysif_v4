                android:layout_marginTop="10dp"
                android:layout_below="@+id/textVerifyPassword"
                android:gravity="right" />
            <TextView
                android:id="@+id/textCheckBox1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="User Agreement and Privacy Policy"
                android:layout_toRightOf="@+id/chekcBox"
                android:textSize="11sp"
                android:layout_below="@+id/textCheckBox"
                android:layout_alignLeft="@+id/textCheckBox"
                android:gravity="right" />
            <Button
                android:id="@+id/buttonSubmit"
                android:text="@string/button_submit"
                android:layout_below="@+id/textCheckBox1"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                style="@style/button_large" />
