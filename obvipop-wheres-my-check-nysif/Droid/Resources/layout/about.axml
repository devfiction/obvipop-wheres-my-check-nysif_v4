<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="17dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="77dp"
            android:id="@+id/relativeLayout1"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            tools:ignore="UselessParent">
            <ImageView
                android:id="@+id/imgPath"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:layout_toEndOf="@+id/imgLogo"
                android:layout_toRightOf="@+id/imgLogo"
                android:contentDescription="@string/headerdivider"
                android:src="@drawable/path_2" />
            <ImageView
                android:id="@+id/imgLogo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="12dp"
                android:contentDescription="@string/headerlogo_change"
                android:src="@drawable/nysif_logo_top_bar" />
            <TextView
                android:id="@+id/textView"
                style="@style/header_title"
                android:layout_centerVertical="true"
                android:text="@string/where_s_my_check" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/relativeLayout1"
            android:layout_marginBottom="17dp">
            <TextView
                android:id="@+id/textAbout"
                style="@style/new_user_registration"
                android:text="About, Contact, Legal"
                android:layout_marginTop="21dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />
            <TextView
                android:id="@+id/textAboutExplanation"
                android:textSize="14dp"
                android:text="2017NYSIF"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:layout_below="@+id/textAbout" />
            <TextView
                android:id="@+id/textAboutAppVersion"
                android:textSize="14dp"
                android:text="App Version 1.2.2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:layout_below="@+id/textAboutExplanation" />
            <TextView
                android:id="@+id/textAboutRateApp"
                android:textSize="14dp"
                android:text="Rate the App &gt;"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:textColor="@color/nysif_red"
                android:layout_below="@+id/textAboutAppVersion" />
            <ImageView
                android:id="@+id/divider1A"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/divider"
                android:scaleType="fitXY"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/textAboutRateApp" />
            <TextView
                android:id="@+id/textContactUs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Contact Us"
                android:textSize="20sp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/divider1A"
                android:gravity="center" />
            <TextView
                android:id="@+id/textContactUsExplanation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="For inquiries or assistance please call:"
                android:textSize="14sp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/textContactUs"
                android:gravity="center" />
            <TextView
                android:id="@+id/textContactUsPhoneNumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="877-435-7743"
                android:textSize="14sp"
                android:layout_marginLeft="25dp"
                android:layout_below="@+id/textContactUsExplanation"
                android:gravity="center" />
            <ImageView
                android:id="@+id/divider2A"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/divider"
                android:layout_below="@+id/textContactUsPhoneNumber" />
            <TextView
                android:id="@+id/textPrivacyStatement"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Privacy Statement"
                android:textSize="20sp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/divider2A"
                android:gravity="center" />
            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="&gt;"
                android:textSize="20sp"
                android:layout_marginRight="25dp"
                android:layout_alignBottom="@+id/textPrivacyStatement"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/divider2A"
                android:gravity="center" />
            <ImageView
                android:id="@+id/divider3A"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/divider"
                android:scaleType="fitXY"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/textPrivacyStatement" />
            <TextView
                android:id="@+id/textTermsConditions"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Terms, Conditions"
                android:textSize="20sp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/divider3A"
                android:gravity="center" />
            <TextView
                android:id="@+id/text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="&gt;"
                android:textSize="20sp"
                android:layout_marginRight="25dp"
                android:layout_alignBottom="@+id/textTermsConditions"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/divider3A"
                android:gravity="center" />
            <ImageView
                android:id="@+id/divider4A"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/divider"
                android:scaleType="fitXY"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/textTermsConditions" />
            <TextView
                android:id="@+id/textFollow"
                style="@style/new_user_registration"
                android:text="Follow. Post. Connect. Watch"
                android:layout_marginTop="21dp"
                android:layout_below="@+id/divider4A"
                android:layout_centerHorizontal="true" />
            <ImageView
                android:id="@+id/imgPathCenter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/textFollow"
                android:src="@drawable/path_2" />
            <ImageView
                android:id="@+id/imgInstagramLogo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toStartOf="@+id/imgPathCenter"
                android:layout_alignBottom="@+id/imgPathCenter"
                android:layout_marginRight="21dp"
                android:layout_below="@+id/textFollow"
                android:src="@drawable/instagram_logo" />
            <ImageView
                android:id="@+id/imgPathLeft"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textFollow"
                android:layout_marginRight="21dp"
                android:layout_toStartOf="@+id/imgInstagramLogo"
                android:src="@drawable/path_2" />
            <ImageView
                android:id="@+id/imgTwitterLogo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toStartOf="@+id/imgPathLeft"
                android:layout_alignBottom="@+id/imgPathCenter"
                android:layout_marginRight="21dp"
                android:layout_below="@+id/textFollow"
                android:src="@drawable/twitter_icon" />
            <ImageView
                android:id="@+id/imgFacebookLogo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/imgPathCenter"
                android:layout_alignBottom="@+id/imgPathCenter"
                android:layout_marginLeft="21dp"
                android:layout_below="@+id/textFollow"
                android:src="@drawable/fb_logo" />
            <ImageView
                android:id="@+id/imgPathRight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textFollow"
                android:layout_marginLeft="21dp"
                android:layout_toEndOf="@+id/imgFacebookLogo"
                android:src="@drawable/path_2" />
            <ImageView
                android:id="@+id/imgYoutubeLogo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/imgPathRight"
                android:layout_alignBottom="@+id/imgPathCenter"
                android:layout_marginLeft="21dp"
                android:layout_below="@+id/textFollow"
                android:src="@drawable/youtube_icon" />
            <Button
                android:id="@+id/buttonBack"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:width="180dp"
                android:background="@color/nysif_teal"
                android:text="Back"
                android:textAllCaps="false"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                android:layout_marginTop="15dp"
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/imgPathCenter"
                android:layout_marginBottom="17dp" />
        </RelativeLayout>
    </RelativeLayout>
</ScrollView>